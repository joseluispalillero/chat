{
    "name": "milsonuno/chat",
    "description": "chat for milsouno.",
    "keywords": ["message", "inbox", "conversations", "chat", "php", "laravel"],
    "homepage": "https://github.com/joseluispalillero/chat",
    "license": "CC(3.0)",
    "authors": [
        {
            "name": "MilSonUno",
            "email": "admin@milsonuno.com"
        }
    ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/contracts": "5.0.* || 5.1.* || 5.2.* || 5.3.* || 5.4.*",
    "illuminate/support": "5.0.* || 5.1.* || 5.2.* || 5.3.* || 5.4.*",
    "sebastian-berc/repositories": "^1.0",
    "nesbot/carbon": "^1.21",
    "pusher/pusher-php-server": "^2.6"
  },
  "require-dev": {
    "graham-campbell/testbench": "^3.1",
    "phpunit/phpunit": "^4.8 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "MilSonUno\\Chat\\": "src/"
    },
    "files": [
      "src/helpers/chat.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MilSonUno\\Chat\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
